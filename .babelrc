{
  "presets": [
    ["@babel/preset-env"]
  ],
  "env": {
    "test": {
      "presets": [
        ["@babel/preset-env", {
          "modules": "commonjs",
          "targets": {
            "node": "current"
          }
        }]
      ]
    },
    "commonjs": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "browsers": [
              "last 2 versions",
              "ie > 10"
            ],
            "node": 6
          },
          "modules": "commonjs"
        }]
      ]
    },
    "es": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "browsers": [
              "last 2 versions",
              "ie > 10"
            ],
            "node": 6
          },
          "modules": false
        }]
      ]
    }
  }
}